<% provide(:title, 'Students') %>

<table>
  <tr>
    <th>First</th>
    <th>Last</th>
    <th>Cell</th>
    <th>Email</th>
    <th>Birthdate</th>
    <th>Grade</th>
    <th>Customer</th>
  </tr>
  <% for student in @students %>
    <tr>
      <td><%= student.first %></td>
      <td><%= student.last %></td>
      <td><%= student.cell %></td>
      <td><%= student.email %></td>
      <td><%= student.birthdate %></td>
      <td><%= student.grade %></td>
      <td><%= student.customer_id %></td>
      <td><%= link_to "Edit", edit_student_path(student) %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Student", new_student_path %></p>
